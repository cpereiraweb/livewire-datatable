    "homepage": "https://github.com/luanfreitasdev/livewire-datatable",
    "license": "MIT",
    "authors": [
        {
            "name": "Luan Freitas",
            "email": "luanfreitasgoncalves@gmail.com",
            "role": "Developer"
        }
